{
  "name": "ssq-generator",
  "version": "0.1.3",
  "description": "用于生成中国福彩双色球的TypeScript实用程序库",
  "keywords": [
    "ssq",
    "lottery",
    "china-lottery",
    "double-color-ball",
    "typescript"
  ],
  "license": "MIT",
  "author": "Liang",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
